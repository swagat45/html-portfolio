#include <iostream>

int main() {
    try {
        // Some code that might throw different types of exceptions
        int numerator, denominator;
        std::cout << "Enter numerator: ";
        std::cin >> numerator;
        std::cout << "Enter denominator: ";
        std::cin >> denominator;

        if (denominator == 0) {
            throw "Division by zero exception";
        }

        if (numerator % denominator != 0) {
            throw 1.5; // Throw a double type exception
        }

        int result = numerator / denominator;
        std::cout << "Result of division: " << result << "\n";
    }
    catch (const char* errorMessage) {
        std::cerr << "Caught exception: " << errorMessage << "\n";
    }
    catch (double exceptionValue) {
        std::cerr << "Caught double exception: " << exceptionValue << "\n";
    }
    catch (...) {
        std::cerr << "Caught unknown exception\n";
    }

    return 0;
}
